<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bleum.canton.sample.entity">
	<class name="ProductStorage" table="T_PRODUCT_STORAGE">
		<composite-id name="id" class="com.bleum.canton.sample.entity.ProductStorageId">
			<key-property name="productId" type="java.lang.String" column="PRODUCT_ID" />
			<key-property name="storageId" type="java.lang.String" column="STORAGE_ID" />
		</composite-id>
	</class>
</hibernate-mapping>
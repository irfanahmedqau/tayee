<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bleum.canton.sample.entity">
	<class name="Product" table="T_PRODUCT">
		<id name="id" column="ID" type="java.lang.String">
			<generator class="uuid" />
		</id>
		<property name="productName" type="java.lang.String" column="PRODUCT_NAME" />
		<property name="skuId" type="java.lang.String" column="SKU_ID" />
	</class>
</hibernate-mapping>